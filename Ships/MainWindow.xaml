        <Label x:Name="label3" Content="Y: " HorizontalAlignment="Left" Margin="438,382,0,0" VerticalAlignment="Top" Width="25" Height="23"/>
        <TextBox x:Name="xTextBox" HorizontalAlignment="Left" Height="23" Margin="468,359,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" IsEnabled="False"/>
        <TextBox x:Name="yTextBox" HorizontalAlignment="Left" Height="23" Margin="468,382,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" IsEnabled="False"/>
