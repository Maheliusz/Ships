        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="640">
    <Grid Margin="0,-28,0,-4">
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="43,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="236" Width="211" RenderTransformOrigin="0.494,0.506" FontSize="16" FontStretch="Expanded"/>
